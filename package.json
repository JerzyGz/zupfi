{
  "name": "zupfi",
  "scripts": {
    "db:touch": "wrangler d1 execute expenses-db --local --command='SELECT 1'",
    "db:generate": "drizzle-kit generate --config=drizzle-dev.config.ts",
    "db:generate:custom": "drizzle-kit generate --custom --config=drizzle-dev.config.ts",
    "db:migrate": "drizzle-kit migrate --config=drizzle-dev.config.ts",
    "db:migrate:prod": "drizzle-kit migrate --config=drizzle-prod.config.ts",
    "db:setup": "pnpm run db:touch && pnpm run db:generate && pnpm run db:migrate",
    "build": "tsc -b && vite build",
    "cf-typegen": "wrangler types --include-env=false",
    "check": "tsc && vite build && wrangler deploy --dry-run",
    "deploy": "pnpm run build && wrangler deploy",
    "dev": "vite",
    "lint": "eslint .",
    "preview": "pnpm run build && vite preview",
    "dev:ngrok": "ngrok http --url=conversely-normal-urchin.ngrok-free.app 5173"
  },
